<%- include('header') -%>

        <div class="box" id="heading">
            <h1> <%= day %></h1>
        </div>

        <div class="box">
            <% for ( let i =0; i<items.length; i++ ) { %>
                <div class="item">
                    <input type="checkbox">
                    <p> <%= items[i] %></>
                    </div>
                    <% } %>

                    <form class="item" action="/" method="post">
                        <input type="text" name="newItem" placeholder="New Item"
                            autofocus>
                        <button type="submit" name="list" value=<%= day
                            %>>Add</button>
                    </form>
                </div>
                <%- include('footer') -%>